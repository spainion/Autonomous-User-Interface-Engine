{
  "agents": {
    "codex": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.7,
      "max_tokens": 2048,
      "purpose": "Code generation and analysis",
      "use_context_engine": true,
      "context_recall_limit": 5
    },
    "ui_designer": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.8,
      "max_tokens": 1500,
      "purpose": "UI/UX design and component generation",
      "use_context_engine": true,
      "context_recall_limit": 5
    },
    "reasoning_agent": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.6,
      "max_tokens": 2048,
      "purpose": "Logic reasoning and planning",
      "use_context_engine": true,
      "context_recall_limit": 10
    }
  },
  "context_engine": {
    "enabled": true,
    "shared_context": true,
    "embedding_backend": "openai",
    "embedding_model": "text-embedding-3-small",
    "clustering_method": "kmeans",
    "default_similarity_threshold": 0.5,
    "max_context_nodes": 10000,
    "features": {
      "deduplication": true,
      "vector_similarity": true,
      "spatial_queries": true,
      "clustering": true,
      "path_finding": true
    }
  },
  "openrouter": {
    "base_url": "https://openrouter.ai/api/v1",
    "api_key_env": "OPENROUTER_API_KEY",
    "fallback_models": [
      "anthropic/claude-3-opus",
      "openai/gpt-4-turbo",
      "meta-llama/llama-3-70b-instruct"
    ]
  },
  "codex": {
    "engine": "code-davinci-002",
    "use_for": [
      "code_completion",
      "code_generation",
      "code_review",
      "refactoring"
    ],
    "settings": {
      "temperature": 0.3,
      "max_tokens": 1024,
      "top_p": 1,
      "frequency_penalty": 0,
      "presence_penalty": 0
    }
  },
  "features": {
    "auto_reasoning": true,
    "multi_agent_collaboration": true,
    "context_preservation": true,
    "ui_auto_generation": true,
    "context_aware_responses": true,
    "agent_memory_sharing": true
  }
}
