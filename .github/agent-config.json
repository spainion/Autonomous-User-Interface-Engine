{
  "agents": {
    "codex": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.7,
      "max_tokens": 2048,
      "purpose": "Code generation and analysis",
      "use_context_engine": true,
      "context_recall_limit": 5,
      "enhanced": true,
      "network_enabled": true,
      "batch_size": 10,
      "parallel_processing": true
    },
    "ui_designer": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.8,
      "max_tokens": 1500,
      "purpose": "UI/UX design and component generation",
      "use_context_engine": true,
      "context_recall_limit": 5,
      "enhanced": true,
      "network_enabled": true,
      "batch_size": 10,
      "parallel_processing": true
    },
    "reasoning_agent": {
      "enabled": true,
      "model": "gpt-4",
      "temperature": 0.6,
      "max_tokens": 2048,
      "purpose": "Logic reasoning and planning",
      "use_context_engine": true,
      "context_recall_limit": 10,
      "enhanced": true,
      "network_enabled": true,
      "batch_size": 10,
      "parallel_processing": true
    }
  },
  "context_engine": {
    "enabled": true,
    "shared_context": true,
    "embedding_backend": "openai",
    "embedding_model": "text-embedding-3-small",
    "clustering_method": "kmeans",
    "default_similarity_threshold": 0.5,
    "max_context_nodes": 10000,
    "network_enhanced": true,
    "features": {
      "deduplication": true,
      "vector_similarity": true,
      "spatial_queries": true,
      "clustering": true,
      "path_finding": true,
      "api_enrichment": true,
      "cloud_sync": true,
      "real_time_embeddings": true
    }
  },
  "network": {
    "enabled": true,
    "whitelist_all_domains": true,
    "allowed_domains": [
      "openai.com",
      "openrouter.ai",
      "huggingface.co",
      "github.com",
      "googleapis.com",
      "anthropic.com",
      "*.openai.com",
      "*.huggingface.co",
      "*"
    ],
    "timeout_seconds": 30,
    "max_retries": 3,
    "enforce_network_usage": true
  },
  "processing": {
    "batch_enabled": true,
    "default_batch_size": 10,
    "max_batch_size": 100,
    "parallel_execution": true,
    "max_workers": 4,
    "iterative_enhancement": true,
    "default_iterations": 3,
    "continuous_optimization": true
  },
  "openrouter": {
    "base_url": "https://openrouter.ai/api/v1",
    "api_key_env": "OPENROUTER_API_KEY",
    "fallback_models": [
      "anthropic/claude-3-opus",
      "openai/gpt-4-turbo",
      "meta-llama/llama-3-70b-instruct",
      "google/gemini-pro",
      "mistralai/mixtral-8x7b-instruct"
    ],
    "enforce_usage": true
  },
  "codex": {
    "engine": "code-davinci-002",
    "use_for": [
      "code_completion",
      "code_generation",
      "code_review",
      "refactoring",
      "batch_generation",
      "iterative_improvement"
    ],
    "settings": {
      "temperature": 0.3,
      "max_tokens": 1024,
      "top_p": 1,
      "frequency_penalty": 0,
      "presence_penalty": 0
    }
  },
  "features": {
    "auto_reasoning": true,
    "multi_agent_collaboration": true,
    "context_preservation": true,
    "ui_auto_generation": true,
    "context_aware_responses": true,
    "agent_memory_sharing": true,
    "batch_processing": true,
    "iterative_enhancement": true,
    "network_integration": true,
    "continuous_optimization": true,
    "cross_compatibility": true
  },
  "compatibility": {
    "optimize_all_systems": true,
    "ensure_interoperability": true,
    "standardize_interfaces": true,
    "enable_hot_reload": true,
    "support_plugins": true
  }
}
